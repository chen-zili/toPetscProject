#%Module

proc ModulesHelp { } {
    puts stderr "This module sets the path and environment variables for petsc-3.16.1"
    puts stderr "     see https://petsc.org/ for more information      "
    puts stderr ""
}
module-whatis "PETSc - Portable, Extensible Toolkit for Scientific Computation"

set petsc_dir   "/home/chenzili/petsc"
set petsc_arch  "arch-ci-linux-intel-mkl-single"

setenv PETSC_ARCH "$petsc_arch"
setenv PETSC_DIR "$petsc_dir"
prepend-path PATH "/home/chenzili/petsc/arch-ci-linux-intel-mkl-single/bin:/home/chenzili/petsc/bin"
